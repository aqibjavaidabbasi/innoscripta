{
   "compilerOptions": {
      // "target": "ES2020",
      // "target": "esnext",
      // "module": "esnext",
      // "module": "ESNext", 
      "target": "ESNext",
      "outDir": "./dist",
      "resolveJsonModule": true,
      "jsx": "react",
      "module": "ESNext",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "lib": [
         "DOM",
         "ESNext"
      ]
   },
   "include": [
      "src"
   ]
   
}
